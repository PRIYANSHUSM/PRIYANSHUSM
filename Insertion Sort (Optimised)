#this is the optimised insertion sort where elements are shifted and not swapped

def insertionSort(self, alist, n):
        #code here
        for i in range(1,n):
            temp = alist[i]
            j = i
            while j>0 and temp < alist[j-1]:
                alist[j] = alist[j-1]
                alist[j-1] = temp
                j-=1
                
                
                




